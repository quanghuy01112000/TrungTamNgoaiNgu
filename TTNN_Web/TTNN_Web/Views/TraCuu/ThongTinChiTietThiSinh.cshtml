@using TTNN_Web.Models
@model dynamic

@{
    ViewBag.Title = "ThongTinChiTietThiSinh";
    Layout = "~/Views/Shared/_LayoutTraCuu.cshtml";
}

<section class="section db p120">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="tagline-message page-title text-center">
                    <h3>Thông Tin Chi Tiết Thí Sinh</h3>
                    <ul class="breadcrumb">
                        <li><a href="javascript:void(0)">Trung tâm ngoại ngữ</a></li>
                        <li class="active">Thông tin Chi Tiết thí sinh</li>
                    </ul>
                </div>
            </div><!-- end col -->
        </div><!-- end row -->
    </div><!-- end container -->
</section><!-- end section -->

<section class="section gb nopadtop">
    <div class="container">
        <div class="boxed boxedp4">



            <div class="row contactv2">
                <!-- table section -->
                <div class="col-md-6 col-md-offset-4">
                    <div class="small-box">
                        <i class="flaticon-black-graduation-cap-tool-of-university-student-for-head wow fadeIn"></i>
                        <h4>&nbsp;&nbsp;&nbsp;&nbsp;@Model.TenThiSinh</h4>
                        <small>Số báo danh&ensp;: @Model.SBD</small>
                        <small>Phòng thi&emsp;&ensp;&nbsp;: @Model.PhongThi</small>
                        <small>Số điện thoại&nbsp;: @Model.SDT</small>
                        <small>CMND&emsp;&emsp;&nbsp;&emsp;: @Model.cmnd</small>
                        <small>Ngày sinh&emsp;&ensp;&nbsp;: @Model.NgaySinh</small>
                        <small>Trình độ&emsp;&ensp;&emsp;: @Model.TrinhDo</small>
                        <small>Ngày thi&emsp;&ensp;&emsp;: @Model.NgayThi</small>
                        <small>Điểm Nghe&emsp;&nbsp;: <strong>@Model.DiemNghe</strong></small>
                        <small>Điểm Noi&emsp;&emsp;: <strong>@Model.DiemNoi</strong></small>
                        <small>Điểm Đọc&emsp;&emsp;: <strong>@Model.DiemDoc</strong></small>
                        <small>Điểm Viết&emsp;&emsp;: <strong>@Model.DiemViet</strong></small>
                        @*<p><a href="@Url.Action("ChiTietGiayChungNhan", "TraCuu")" target="_blank">Giấy chứng nhận</a></p>*@
                    </div><!-- end small-box -->
                </div>
            </div><!-- end contactv2 -->
            <!-- end row -->
        </div><!-- end container -->
    </div>
</section>



