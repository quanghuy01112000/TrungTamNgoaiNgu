@using TTNN_Web.Models
@model dynamic

@{
    ViewBag.Title = "ThongTinChiTietThiSinh";
    Layout = "~/Views/Shared/_LayoutTraCuu.cshtml";
}

<section class="section db p120">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="tagline-message page-title text-center">
                    <h3>Thông Tin Chi Tiết Thí Sinh</h3>
                    <ul class="breadcrumb">
                        <li><a href="javascript:void(0)">Trung tâm ngoại ngữ</a></li>
                        <li class="active">Thông tin Chi Tiết thí sinh</li>
                    </ul>
                </div>
            </div><!-- end col -->
        </div><!-- end row -->
    </div><!-- end container -->
</section><!-- end section -->

<section class="section gb nopadtop">
    <div class="container">
        <div class="boxed boxedp4">



            <div class="row contactv2 text-center">
                <!-- table section -->
                <div class="col-md-6 col-md-offset-3">
                    <div class="small-box">
                        <i class="flaticon-black-graduation-cap-tool-of-university-student-for-head wow fadeIn"></i>
                        <h4>Trình độ A2</h4>
                        <small>Số báo danh: @Model.SBD</small>
                        <small>Phòng thi:  @Model.PhongThi</small>
                        <small>Ngày thi:  @Model.NgayThi</small>
                        <small>Điểm Nghe: <strong>@Model.DiemNghe</strong></small>
                        <small>Điểm Noi: <strong>@Model.DiemNoi</strong></small>
                        <small>Điểm Đọc: <strong>@Model.DiemDoc</strong></small>
                        <small>Điểm Viết: <strong>@Model.DiemViet</strong></small>
                        @*<p><a href="@Url.Action("ChiTietGiayChungNhan", "TraCuu")" target="_blank">Giấy chứng nhận</a></p>*@
                    </div><!-- end small-box -->
                </div>
            </div><!-- end contactv2 -->
            <!-- end row -->
        </div><!-- end container -->
    </div>
</section>



